[parse_startup]
start=\s*Done
important=true

## See the default craftbukkit.jar.conf for a detailed documentation of the
## format of this file.
[config]
name = PaperSpigot [VERSION]
source = http://jar-v2.freemcserver.net/jar/stable/paperspigot/paperspigot-[VERSION].jar
configSource = http://jar-v2.freemcserver.net/conf/stable/paperspigot/paperspigot-[VERSION].jar.conf
category = Mods

[encoding]
#encode = system
#decode = system
#fileEncoding = latin-1

[start]
command = "{JAVA}" -Xmx{MAX_MEMORY}M -Xms{START_MEMORY}M -XX:MaxPermSize=128M -Djline.terminal=jline.UnsupportedTerminal -Dcom.mojang.eula.agree=true -jar "{JAR}" nogui